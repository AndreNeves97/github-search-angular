{
  "hosting": [
    {
      "target": "default",
      "public": "dist/scalio-challenge",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "github-search-tests-coverage",
      "public": "coverage/scalio-challenge",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
    }
  ]
}
